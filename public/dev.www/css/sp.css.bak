/**************************************************
		sidebar
**************************************************/
#sidebar{
display:none;
}
#bnrarea{
width:100%;
margin:0 auto;
}

#sidebox-live, #sidebox-sche, #sidebox-table {
width:auto;
min-height:300px;
border-left: 1px solid #ccc;
border-right: 1px solid #ccc;
border-bottom: 1px solid #ccc;
margin-top:10px;
}
#sidebox-live .sideboxline, #sidebox-sche .sideboxline, #sidebox-table .sideboxline {
width:100%;
height:5px;
background:#00489b;
}
#sidebox-live h2, #sidebox-sche h2, #sidebox-table h2 {
text-align:center;
color:#00489b;
font-size: 13px;
padding:6px 0;
}

/****** sidebox-live ******/

/**top**/
#sidebox-liveSPTOP #livebtarea{
width:100%;
text-align:center;
}
#sidebox-liveSPTOP #livebtarea .livebt{
text-align:center;
margin-bottom:30px;
margin-left:auto;
margin-right:auto;
}
#sidebox-liveSPTOP #livebtarea .livebt a span{
padding:15px 110px;
background:#00489b;
color:#fff;
font-size: 14px;
line-height: 14px;
text-decoration:none;
}



#sidebox-sche #livebox {
margin:6px;
min-height:300px;
}
#sidebox-live .compe {
text-align:center;
font-size: 12px;
padding:2px 0;
}
#sidebox-live .gameno {
text-align:center;
font-size: 12px;
}
#sidebox-live .gameno .gamedata {
padding:0 0 0 6px;
}
#sidebox-live #match {
position: relative;
width:100%;
margin-top:20px;
}
#sidebox-live #match .home{
width:114px;
text-align:center;
float:left;
}

#sidebox-live #match .score{
width:70px;
text-align:center;
font-size: 12px;
float:left;
}
#sidebox-live #match .away{
width:114px;
text-align:center;
float:right;
}
#sidebox-live #match .home span, #sidebox-live #match .away span{
font-size: 10px;
}
#sidebox-live #match .home img, #sidebox-live #match .away img{
margin:0 0 10px 0;
width:100px;
height:100px;
}
#sidebox-live #match .score{
text-align:center;
font-size: 24px;
/*font-family: Oswald;*/
letter-spacing: 6px;
margin-top:40px;
}
#livebtarea{
width:100%;
text-align:center;
clear:both;
}
#sidebox-live #livebtarea .livebt{
text-align:center;
margin-top:185px;
margin-left:auto;
margin-right:auto;
}
#sidebox-live #livebtarea .livebt a span{
padding:10px 40px;
background:#00489b;
color:#fff;
font-size: 14px;
line-height: 14px;
text-decoration:none;
}





/****** sidebox-sche ******/
#sidebox-sche #schebox {
margin:6px;
}
#sidebox-sche #schebox #match {
width:100%;
margin:6px 0 20px 0;
}
#sidebox-sche #schebox #match .compe {
font-size: 12px;
padding:2px 0;
}
#sidebox-sche #schebox #match .gameno {
font-size: 12px;
margin-bottom:6px;
}
#sidebox-sche #schebox #match .gameno .gamedata {
padding:0 0 0 6px;
}
#sidebox-sche #schebox #match .home{
width:40px;
text-align:center;
float:left;
}
#sidebox-sche #schebox #match .away{
width:40px;
text-align:center;
float:left;
}
#sidebox-sche #schebox #match .hname, #sidebox-sche #schebox #match .aname {
font-size: 10px;
padding:3px 0 0 0;
}
#sidebox-sche #schebox #match .home img, #sidebox-sche #schebox #match .away img{
margin:0 0 10px 0;
width:40px;
height:40px;
}
#sidebox-sche #schebox #match li{
border-bottom: 1px solid #ccc;
padding:10px 0;
margin:10px;
height:85px;
}/*
#sidebox-sche #schebox #match li:last-child{
border-bottom-style:none;
}*/
#sidebox-sche #schebox #match ul{
width:100%;
margin:0 0 10px 0;
}

/*schecompe*/
#schebox #schecompe{
display: inline;
width:100%;
margin:10px;
clear:both;
}
#schebox #schecompe ul{
width:100%;
display: inline;
list-style: none;
}
#schebox #schecompe ul li{
width:100%;
height:32px;
border-bottom: 1px solid #ccc;
}
#schebox #schecompe ul li:last-child{
border-bottom-style:none;
}
#schebox #schecompe ul li a{
width: 280px;
display: block;
padding: 6px;
margin-top: 4px;
text-decoration: none;
text-align: left;
font-size: 12px;
letter-spacing: 0.1em;
}
#schebox #schecompe a .fa-angle-right{
padding:3px 6px 0px 3px;
}



/****** sidebox-table ******/

#sidebox-table table {
width:100%;
font-size: 12px;
}
#sidebox-table table th {
padding: 3px 0 3px 0;
text-align: center;
}
#sidebox-table table td {
text-align: center;
}
#sidebox-table table th.column-1 {
width: 50px;
}
#sidebox-table table th.column-2 {
width: 200px;
padding-left: 12px;
text-align: left;
}
#sidebox-table table th.column-3 {
width: 50px;
}
#sidebox-table table td.column-1 {
width: 50px;
padding: 3px 0 3px 0;
}
#sidebox-table table td.column-2 {
width: 200px;
padding-left: 12px;
text-align: left;
}
#sidebox-table table td.column-3 {
width: 50px;
}

/* 順位の色付け */
#sidebox-table table tr.row-2 td.column-1,
#sidebox-table table tr.row-3 td.column-1,
#sidebox-table table tr.row-4 td.column-1,
#sidebox-table table tr.row-5 td.column-1 {
background: #004586;
color:#fff;
background-position: 0 center;
background-repeat: repeat-x;
}

#sidebox-table table tr.row-6 td.column-1,
#sidebox-table table tr.row-7 td.column-1 {
background: #6f0810;
color:#fff;
background-position: 0 center;
background-repeat: repeat-x;
}

#sidebox-table table tr.row-19 td.column-1,
#sidebox-table table tr.row-20 td.column-1,
#sidebox-table table tr.row-21 td.column-1 {
background:#000;
color:#fff;
background-position: 0 center;
background-repeat: repeat-x;
}
tr.even {
background:#fff;
}
tr.odd {
background:#f0f0f0;
}


/**************************************************
		footer
**************************************************/

#line2{
display:none;
}

#footer{
display:none;
}

#footer_inner{
display:none;
}


/**************************************************
		support
**************************************************/

#footersp #support{
width:auto;
border:1px solid #ccc;
margin-bottom:10px;
}
#footersp #support li {
height:40px;
line-height:40px;
border-bottom:1px solid #ccc;
background-color:#fff;
}
#footersp #support li:last-child {
border-bottom-style:none;
}
#footersp #support li a {
display: block;
text-decoration:none;
padding:0px 0px 0px 5px;
font-size:12px;
}

#footersp #support li.fa{
display: inline-block;
text-align:right;
font-size:12px;
}





/**************************************************
		contentsarea
**************************************************/


#contentsarea{
width:100%;
border-top:2px solid #00489b;
border-left:1px solid #ccc;
border-right:1px solid #ccc;
border-bottom:1px solid #ccc;
margin-bottom:10px;
}

/**************************************************
		plink
**************************************************/


#plink{
margin:10px;
}
#plink .pnavi{
padding: 3px;
display: box;
display: -webkit-box;
display: -moz-box;
box-pack:justify;
-webkit-box-pack:justify;
-moz-box-pack:justify;
margin-bottom:6px;
}
#plink .pnavi .pleft,
#plink .pnavi .pright{
font-size:14px;
}
#plink .pnavi .pcenter{
box-flex:1;-webkit-box-flex:1; -moz-box-flex:1;
}

/**************************************************
		singlenews
**************************************************/

#singlenews{
margin:10px;
border-top:1px solid #ccc;
border-bottom:1px solid #ccc;
height:auto;
}

#singlenews .newsheader{
margin:15px 0;
display: box;
display: -webkit-box;
display: -moz-box;
}


#singlenews .newsheader .newsdate{
font-size:12px;
line-height:16px;
color:#00489b;
}

#singlenews .newsheader .newscate{
margin-left:10px;
padding-left:10px;
font-size:12px;
line-height:16px;
border-left:1px solid #ccc;
}

#singlenews .newstitle{
margin:0 0 15px 6px;
font-size:16px;
line-height:20px;
letter-spacing: 0.1em;
}

#singlenews .newstext{
border-top:1px solid #ccc;
padding:15px 0;
font-size:14px;
line-height:20px;
letter-spacing: 0.1em;
}


/**************************************************
		archive-news
**************************************************/

#archive-news{
margin:10px;
border-top:1px solid #ccc;
border-bottom:1px solid #ccc;
height:auto;
}


#archive-news .newslist{
