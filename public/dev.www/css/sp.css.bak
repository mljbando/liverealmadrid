}

#archive-news .newslist li{
margin: 10px 0;
min-height:70px;
border-bottom:1px solid #ccc;
}

#archive-news .newslist li:last-child{
border-bottom-style:none;
}

#archive-news .newslist .newsdate{
font-size:12px;
line-height:16px;
color:#00489b;
float:left;
}


#archive-news .newslist .newscate{
margin-left:120px;
padding-left:6px;
font-size:12px;
line-height:16px;
border-left:1px solid #ccc;
}

#archive-news .newslist .newstitle{
margin-top:6px;
padding-left:6px;
font-size:14px;
line-height:20px;
clear:both;
}

