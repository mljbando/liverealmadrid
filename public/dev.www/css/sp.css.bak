@charset "utf-8";
/* CSS Document */

/**************************************************
		base
**************************************************/
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,
form,input,p,blockquote,th,td {
margin:0;
padding:0;
}  
img{
border:0;
vertical-align: top;
}
body{
min-width:320px;
font-family: "メイリオ","Meiryo",verdana,"ヒラギノ角ゴ Pro W3","Hiragino Kaku Gothic Pro",Osaka,"ＭＳ Ｐゴシック","MS PGothic",Sans-Serif;
color:#2f2f2f;
background:#fff;
margin:0;
}

a:link,
a:visited,
a:active{
color:#333;
text-decoration:none;
}
a:hover{
color:#069;
text-decoration:none;
}
ul,ol{
list-style: none;
}
.clearfix:after{  
visibility:hidden;
height:0;
display: block;
font-size: 0;
content: " ";
clear: both;
}


#sp{
display:none;
}

#pageTop{
background:#666;
color:#FFF;
padding:20px;
position:fixed;
bottom:20px;
right:20px;

}
#pageTop:hover{
background:#ccc;
}




/**************************************************
		sidr
**************************************************/

.sidr{
display:none;
position:absolute;
position:fixed;
top:0;
left:0;
height:100%;
z-index:999999;
width:260px;
overflow-x:none;
overflow-y:auto;
font-size:15px;
background:#2b2b2b;
color:#fff;
}

.sidr .sidr-inner{
padding:0 0 15px;
}

.sidr .sidr-inner>p{
margin-left:15px;
margin-right:15px;
}

.sidr.right{
left:auto;
right:-260px;
}

.sidr.left{left:-260px;
right:auto;
}

.sidr h1,.sidr h2,.sidr h3,.sidr h4,.sidr h5,.sidr h6{
font-size:20px;
font-weight:normal;
padding:0 15px;
margin:0 0 5px;
color:#fff;
line-height:24px;
}

.sidr p{
font-size:13px;
margin:0 0 12px;
}
.sidr p a{
color:#fff;
}

.sidr>p{
margin-left:15px;
margin-right:15px;
}

.sidr ul{
display:block;
margin:0 0 15px;
padding:0;
}
.sidr ul li{
display:block;
margin:0;
line-height:48px;
border-bottom:1px solid #151515;
}

.sidr ul li:hover,
.sidr ul li.active,
.sidr ul li.sidr-class-active{
border-top:none;
line-height:48px;
color:#fff;
}


.sidr ul li a,
.sidr ul li span{
padding:0 15px;
display:block;
text-decoration:none;
color:#fff;
}

.sidr ul li ul{
border-bottom:none;
margin:0;
}

.sidr ul li ul li{
line-height:40px;
font-size:13px;
}

.sidr ul li ul li:last-child{
border-bottom:none;
}

.sidr ul li ul li:hover,
.sidr ul li ul li.active,
.sidr ul li ul li.sidr-class-active{
border-top:none;
line-height:40px;
}




/**************************************************
		header
**************************************************/
#line{
width:100%;
height:5px;
background:#00489b;
}

#headersp{
position:absolute;
width:100%;
height:50px;
border-bottom:1px solid #ccc;
background:#FFF;
top:0;
z-index:2
}


#headersp .menu{
margin:0 0 0 10px;
}

#simple-menu{
float:left;
}

#simple-menu .smenu{
font-size:32px;
font-weight:bold;
}

.logosp{
padding:8px 0px 0px 60px ;
float:left;

}

.loginbt{
font-size:30px;
padding:8px 15px 0 0;
text-align:right;
}




#header{
display:none;
}

/**************************************************
		subnav
**************************************************/
#subnav{
display:none;
}

/**************************************************
		nav
**************************************************/
#nav{
display:none;
}

/**************************************************
		visual
**************************************************/
#visual{
display:none;
}

/**************************************************
		contents
**************************************************/
#container{
width:100%;
margin:0;
overflow: hidden;
}
#contents{
width:auto;
margin:62px 5px 110px 5px;
padding:0;
}
#contentsleft, #contentsright, #footersp{
width:auto;
margin:0px 5px 0 5px;
padding:0;
}


/**************************************************
		news
**************************************************/
/*tabmenu*/
#news{
width:100%;
margin:0;
}
#news .tabmenu{
width:50px;
margin: 0; 
padding: 0; 
list-style: none;
float:right;
}
#news .tabmenu li{
list-style-type: none;
border-right:1px solid #ccc;
border-left:1px solid #ccc;
border-bottom:1px solid #ccc;
width: 50px;
height: 60px;
}
#news .tabmenu li:first-child{
border-top:1px solid #ccc;
}
#news .tabmenu li a {
height: 45px;
padding-top:15px;
display: block;
word-wrap:break-word;
background:#fff;
color:#666;
text-align: center;
text-decoration: none;
font-size: 10px;
line-height:16px;
}
#news .tabmenu li.active a{
background:#00489b;
color:#fff;
}
/*content*/
#news .content {
overflow: hidden;
width: auto;
height:365px;
border-top:1px solid #ccc;
border-bottom:1px solid #ccc;
border-left:1px solid #ccc;
}
#news .tabbox {
width: 100%;
overflow: hidden;
margin:3px 6px 15px 6px;
}
/*newsaeraleft*/
.newsaeraleft{
display:none;
}
/*newsaeraright*/
.newsaeraright{
width:96%;
margin:0 6px 0 0;
padding:0;
float: right;
overflow: hidden;
}
.newsaeraright ul{
width:90%;
display: inline;
list-style: none;
}
.newsaeraright ul li{
display: inline-block;
width:96%;
height:40px;
border-bottom: 1px solid #ccc;
}
.newsaeraright ul li a{
display: block;
padding: 11px 8px 0px 8px;
text-decoration: none;
text-align: left;
font-size: 13px;
letter-spacing: 0.1em;
overflow: hidden;
white-space: nowrap;
text-overflow: ellipsis;
}
.newsaeraright .read-next{
text-align: right;
margin:7px 25px 0 0;
}
.newsaeraright .read-next a{
font-size: 12px;
padding:0;
}
.newsaera #newsaeraright ul li a .fa-angle-right{
padding:3px 10px 0px 0;
}






/**************************************************
		berlive
**************************************************/
#berliveT{
width:auto;
height:30px;
margin:10px 0 0 0;
}
#berliveT .title{
padding:8px 0px 8px 12px;
background:#00489b;
color:#fff;
font-size: 14px;
line-height: 14px;
}
#berlive {
width:auto;
/*height:500px;*/
overflow: visible;
border-left: 1px solid #ccc;
border-right: 1px solid #ccc;
}
#berlive ul {
margin: 0 10px 0 10px;
}
#berlive ul li {
position: relative;
min-height:150px;
border-bottom: 1px solid #ccc;
padding-bottom:6px;
padding-top:6px;
}
#berlive ul li:last-child{
border-bottom-style:none;
}
#berlive ul li p.photo {
width: 100%;
height: auto;
overflow: hidden;
top: 0;
left: 0;
}
#berlive ul li p.photo img {
width: 100%;
}
#berlive ul li .berlive-data{
color:#00489b;
font-size: 15px;
padding-bottom:1px;
}
#berlive ul li .berlive-text {
color:#666666;
font-size: 12px;
line-height:16px;
letter-spacing: 0.1em;
padding-bottom:4px;
}

#nextarrowout{
display:none;
}


#SPnextarrowout {
width:auto;
padding:10px 0;
color:#fff;
font-size: 14px;
line-height: 14px;
border-top: 1px solid #ccc;
border-right: 1px solid #ccc;
border-left: 1px solid #ccc;
border-bottom: 1px solid #ccc;
}
#SPnextarrowout .arrow {
width:70px;
margin-left: auto;
}
#SPnextarrowout .arrow a{
font-size: 12px;
line-height: 12px;
}
#SPnextarrowout .arrow a .fa-angle-right{
padding:3px 6px 0px 3px;
}






/**************************************************
		timeline
**************************************************/

#timeline{
display:none;
}

#timelinesp{
margin-top:10px;
width:86%;
margin-right:6%;
margin-left:6%;
text-align:center;
}

