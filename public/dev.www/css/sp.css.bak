/*----------------------------------------------------------------------------------
				match
------------------------------------------------------------------------------------*/


#matchdetail {
	width: 100%;
	margin: 0;
	overflow:hidden;
	background:#fff;
}
#matchdetail .matchdate {
	width: 100%;
	text-align:center;
	font-size:12px;
	margin:0;
	padding-top:3px;
}

#matchdetail .matchcompe {
	width: 100%;
	text-align:center;
	font-size:12px;
	margin:0;
}

#matchdetail .matchmenber {
	width: 100%;
	text-align:center;
	font-size:12px;
	margin:0;

}

#matchdetail .matchstatus {
	width: 100%;
	text-align:center;
	font-size:12px;
	margin:0;
}



#matchsbox1 {
	display: box;
	display: -webkit-box;
	display: -moz-box;

	box-pack:justify;
	-webkit-box-pack:justify;
	-moz-box-pack:justify;

	box-align:center;
	-webkit-box-align:center;
	-moz-box-align:center;

	border-top: 1px solid #e7e7e7;
	border-bottom: 1px solid #e7e7e7;
	margin:6px;
}

#matchsbox1 .Hteam {
	width:38%;
	font-size:12px;
	margin:0;
	padding-left:3px;
}

#matchsbox1 .Hscore {
	width:7%;
	font-size:20px;
	font-weight:bold;
	margin:0;
}

#matchsbox1 .score {
	text-align:center;
	width:10%;
	font-size:14px;
	margin:0;
}

#matchsbox1 .Ascore {
	text-align:right;
	width:7%;
	font-size:20px;
	font-weight:bold;
	margin:0;
}

#matchsbox1 .Ateam {
	text-align:right;
	width:38%;
	font-size:12px;
	margin:0;
	padding-right:3px;
}



#matchsbox2 {
	display: box;
	display: -webkit-box;
	display: -moz-box;

	box-pack:justify;
	-webkit-box-pack:justify;
	-moz-box-pack:justify;

	border-top: 1px solid #e7e7e7;
	border-bottom: 1px solid #e7e7e7;
	margin:6px;
}

#matchsbox2 .Hresult{
	width:44%;
	font-size:11px;
	color:#393939;
	margin:0;
	padding-left:3px;
}

