#comment_box li{
/*	clear: both; */
/*	padding: 12px; */
	padding: 12px 12px 12px 12px; 
/*	border-top: 1px solid #ccc; */
	border-bottom: 1px solid #ccc;
	margin: 0;
	overflow:hidden;
}
#comment_box .comment-body{
	margin:0;
}
#comment_box ol.commets-list{
	width:auto;
	margin: 0;
/*	border-top: 1px solid #ccc; */
	border-right : 1px solid #ccc;
	border-left: 1px solid #ccc;
/*	border-bottom: 1px solid #ccc; */
	padding: 0;
	overflow:hidden;
}

#comment_box ul.children li{
	min-width:240px;
	margin: 12px 0 0 0 ;
	border-top: 1px solid #ccc;
	border-right : 1px solid #ccc;
	border-left: 1px solid #ccc;
	position:relative;
	overflow:visible;
	float:left;
	font-size: 12px;
	box-sizing : border-box;
	width:100%;
	background-color: #cfedff; 
	border-radius: 3px;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px; 
}
#comment_box ul.children{
	clear: both; 
	overflow:hidden;
}
#comment_box ul.children li:after{
	content: "";
	position: absolute;
	top: -9px;
	left: 12px;
	display: block;
	border-style: solid;
	border-width: 0 8px 10px 8px;
	border-color: transparent transparent #cfedff transparent;
}
#comment_box ul.children li:before{
	content: "";
	position: absolute;
	top: -10px;
	left: 12px;
	display: block;
	border-style: solid;
	border-width: 0 8px 10px 8px;
	border-color: transparent transparent #ccc transparent;
}

